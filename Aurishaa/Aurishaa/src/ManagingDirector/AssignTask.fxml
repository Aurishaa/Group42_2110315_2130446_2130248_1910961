      <Label layoutX="190.0" layoutY="19.0" prefHeight="36.0" prefWidth="220.0" text="Assign Task">
         <font>
            <Font name="Arial Bold Italic" size="31.0" />
         </font>
      </Label>
      <Button layoutX="39.0" layoutY="380.0" mnemonicParsing="false" onAction="#goBackToDashboardButtonOnClick" prefHeight="31.0" prefWidth="65.0" text="Back" />
      <Label layoutX="14.0" layoutY="73.0" text="Whom would you like to  assign task to?">
