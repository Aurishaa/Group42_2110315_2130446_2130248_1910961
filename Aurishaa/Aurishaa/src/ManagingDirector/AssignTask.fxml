<AnchorPane id="AnchorPane" prefHeight="438.0" prefWidth="600.0" style="-fx-background-color: lavender;" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.171" fx:controller="ManagingDirector.AssignTaskController">
