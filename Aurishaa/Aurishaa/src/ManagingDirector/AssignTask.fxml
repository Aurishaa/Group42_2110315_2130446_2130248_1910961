      <TextArea fx:id="taskDetailsTextArea" layoutX="336.0" layoutY="164.0" prefHeight="200.0" prefWidth="237.0" />
      <Button layoutX="444.0" layoutY="380.0" mnemonicParsing="false" onAction="#assignTaskButtonOnClick" prefHeight="31.0" prefWidth="105.0" text="Assign Task" />
      <Label layoutX="234.0" layoutY="118.0" text="Due Date:">
         <font>
            <Font name="System Bold Italic" size="16.0" />
         </font>
      </Label>
      <DatePicker fx:id="duedateDatePicker" layoutX="335.0" layoutY="115.0" />
   </children>
</AnchorPane>
