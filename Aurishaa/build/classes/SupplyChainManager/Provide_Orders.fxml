<AnchorPane id="AnchorPane" prefHeight="509.0" prefWidth="672.0" style="-fx-background-color: beige;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="SupplyChainManager.Provide_OrdersController">
